  <Variable
    Name="p_wo_number"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
