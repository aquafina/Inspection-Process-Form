  </Variable>
  <SQLQuery>
    <![CDATA[select  batch_no, header.buyer, job.job_number, job.job_qty, job.output_item_code item_code, job.output_item_desc item_desc
  from PWC_ODM_MASTER_SHT_PRCSNG_HDR header, 
  PWC_ODM_MASTER_SHT_PRCSNG_lINE line,
  PWC_ODM_MASTER_SHT_JOB_PRCSNG job
  where header.header_id = line.header_id
  and line.line_id = job.line_id 
  and job.attribute1 = 'Finishing'
  and to_char(header.work_order_number) = nvl(:p_wo_number,to_char(header.work_order_number))]]>
  </SQLQuery>
