  <SQLQuery>
    <![CDATA[select    wdj.attribute5 batch_no, 
          prc_header.buyer, 
          wdj.attribute1 work_order_number,
          wdj.attribute11 pp_number,
          ooha.order_number sale_order_no,
          we.WIP_ENTITY_NAME job_number, 
          wdj.start_quantity job_qty,  
          msi.CONCATENATED_SEGMENTS item_code, 
          msi.DESCRIPTION item_desc,
          prg_headers.finish,
          gd_headers.finish_gsm, 
          gd_headers.WEAVE_STYLE
  from wip_entities we,
        wip_discrete_jobs wdj,
        oe_order_headers_all ooha,
        PWC_ODM_MASTER_SHT_PRCSNG_HDR prc_header,
        mtl_system_items_kfv msi,
        pwc_om_processing_prg_hdr prg_headers,
        pwc_om_processing_prg_lns prg_lines,
        PWC_OM_GREIGE_DEMOND_HEADER gd_headers
  where  we.wip_entity_id = wdj.wip_entity_id
  and wdj.attribute12 = ooha.header_id
  and nvl(wdj.attribute1,'-1') = nvl(prc_header.WORK_ORDER_NUMBER,'-1') 
  and wdj.primary_item_id = msi.INVENTORY_ITEM_ID
  and wdj.organization_id = msi.ORGANIZATION_ID
  and nvl(wdj.attribute11,'-1') = nvl(prg_headers.PROCESSING_PRG_NO,'-1')
  and prg_headers.pp_header_id = prg_lines.pp_header_id
  and prg_lines.gd_number = gd_headers.gd_no
  and wdj.attribute13 = 'FIN'
  and wdj.attribute_category = 'Job Details'
  and wdj.quantity_completed != 0
  and we.wip_entity_id not in (select  nvl(job_id,0) from PWC_INSPECTION_PROCESS_HEADER)
   and wdj.organization_id = nvl(:p_org_id, wdj.organization_id) 
  and nvl(wdj.attribute1,'-1') = nvl(:p_wo_number,nvl(wdj.attribute1,'-1'))]]>
  </SQLQuery>
